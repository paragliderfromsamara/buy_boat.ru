<p id="notice"><%= notice %></p>

<h1>Boat Parameter Values</h1>

<table>
  <thead>
    <tr>
      <th>Boat type</th>
      <th>Boat parameter type</th>
      <th>Bool value</th>
      <th>Integer value</th>
      <th>Float value</th>
      <th>String value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @boat_parameter_values.each do |boat_parameter_value| %>
      <tr>
        <td><%= boat_parameter_value.boat_type_id %></td>
        <td><%= boat_parameter_value.boat_parameter_type_id %></td>
        <td><%= boat_parameter_value.bool_value %></td>
        <td><%= boat_parameter_value.integer_value %></td>
        <td><%= boat_parameter_value.float_value %></td>
        <td><%= boat_parameter_value.string_value %></td>
        <td><%= link_to 'Show', boat_parameter_value %></td>
        <td><%= link_to 'Edit', edit_boat_parameter_value_path(boat_parameter_value) %></td>
        <td><%= link_to 'Destroy', boat_parameter_value, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Boat Parameter Value', new_boat_parameter_value_path %>
