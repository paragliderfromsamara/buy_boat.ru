<p id="notice"><%= notice %></p>

<h1>Manufacturers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Www</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Logo</th>
      <th>Creator</th>
      <th>Updator</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @manufacturers.each do |manufacturer| %>
      <tr>
        <td><%= manufacturer.name %></td>
        <td><%= manufacturer.www %></td>
        <td><%= manufacturer.email %></td>
        <td><%= manufacturer.phone %></td>
        <td><%= manufacturer.logo %></td>
        <td><%= manufacturer.creator_id %></td>
        <td><%= manufacturer.updator_id %></td>
        <td><%= link_to 'Show', manufacturer %></td>
        <td><%= link_to 'Edit', edit_manufacturer_path(manufacturer) %></td>
        <td><%= link_to 'Destroy', manufacturer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Manufacturer', new_manufacturer_path %>
