<p id="notice"><%= notice %></p>

<h1>Manufacturers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Www</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Logo</th>
      <th>Creator</th>
      <th>Updater</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trademarks.each do |trademark| %>
      <tr>
        <td><%= trademark.name %></td>
        <td><%= trademark.www %></td>
        <td><%= trademark.email %></td>
        <td><%= trademark.phone %></td>
        <td><%= trademark.logo %></td>
        <td><%= trademark.creator_id %></td>
        <td><%= trademark.updater_id %></td>
        <td><%= link_to 'Show', trademark %></td>
        <td><%= link_to 'Edit', edit_trademark_path(trademark) %></td>
        <td><%= link_to 'Destroy', trademark, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trademark', new_trademark_path %>
